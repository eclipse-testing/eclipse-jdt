TEST STEPS

1. Open Fragment Project Overview;
2. Click on "Dependencies:" link;
3. Right-click C dependency and select "Open" command in a context menu;
4. Make sure that C project overview is opened. 

------=_.description-216f885c-d591-38ce-8ea2-e4f8cb4d6ffa--
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "FP/META-INF/MANIFEST.MF" | double-click
with [get-editor FP] {
    get-tab-folder | get-tab-item Dependencies | click
    get-section "Required Plug-ins" | get-table | select "C \\(1.0.0\\)" | get-menu Open | click
}
get-editor C | get-section "General Information" | get-editbox -after [get-label "ID:"] | set-focus
with [get-tab-folder -index 1] {
    get-property activePage | equals C | verify-true
    get-property tabCount | equals 2 | verify-true
}
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
