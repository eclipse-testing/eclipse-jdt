------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

with [get-view "Package Explorer" | get-tree] {
    select "MyJavaProject/src/\\(default package\\)/AllTests.java" | get-menu Copy | click
    select "MyJavaProject/src/\\(default package\\)" | get-menu Paste | click
}
with [get-window "Name Conflict"] {
    get-editbox -after [get-label "Enter a new name for \'AllTests\':"] | set-focus
    get-button OK | click
}
get-view "Package Explorer" | get-tree | select "MyJavaProject/src/\\(default package\\)/CopyOfAllTests.java" 
    | double-click
get-editor "CopyOfAllTests.java" | get-editbox | get-property text | contains "@SuiteClasses({ TC1.class })\n"
    | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
