------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | get-menu "New/Other..." | click
with [get-window New] {
    get-tree | select "Plug-in Development/Fragment Project"
    get-button "Next >" | click
}
with [get-window "New Fragment Project"] {
    get-editbox -after [get-label "Project name:"] | set-text FP
    get-button "Next >" | click
    with [get-group Properties | get-editbox -after [get-label "Version:"]] {
        set-text "1.0.2"
        set-text "1.2"
    }
    with [get-group "Host Plug-in"] {
        get-editbox -after [get-label "Plug-in ID:"] | click-text
        get-button "Browse..." | click
    }
    with [get-window "Plug-in Selection"] {
        get-editbox -after [get-label "Select a Plug-in:"] | set-text plugin
        get-table | select "PluginProject \\(1.0.0.qualifier\\)"
        get-button OK | click
    }
    get-button Finish | click
}
get-editor FP | get-section "General Information" | get-editbox -after [get-label "Version:"] | get-property text 
    | equals "1.2" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
